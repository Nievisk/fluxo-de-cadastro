generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id         String @id @unique @db.Uuid
  first_name String @db.VarChar(50)
  last_name  String @db.VarChar(50)
  email      String @unique @db.VarChar(250)
}